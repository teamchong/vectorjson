{
  "name": "vectorjson",
  "version": "0.1.0",
  "description": "O(n) WASM SIMD streaming JSON parser — 900-2000× faster partial JSON for AI SDKs vs Vercel/Anthropic/TanStack",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build:zig": "zig build -Doptimize=ReleaseSmall",
    "build:opt": "npx wasm-opt -Oz --strip-producers dist/engine.wasm -o dist/engine.wasm --enable-simd --enable-bulk-memory --enable-nontrapping-float-to-int --enable-sign-ext --enable-mutable-globals",
    "build:js": "bun x tsc",
    "build": "bun run build:zig && bun run build:opt && bun run build:js",
    "test": "bun test/phase1.mjs && bun test/phase2.mjs && bun test/parse-result.mjs && bun test/is-complete.mjs && bun test/partial-json.mjs && bun test/zerocopy.mjs && bun test/standards.mjs && bun test/event-parser.mjs && bun test/live-doc.mjs",
    "bench": "bun --expose-gc bench/parse-stream.mjs",
    "bench:parse": "bun --expose-gc bench/parse-stream.mjs",
    "bench:e2e": "bun --expose-gc bench/end-to-end.mjs",
    "bench:ai": "cd bench/ai-parsers && bun --expose-gc bench.mjs"
  },
  "keywords": [
    "json",
    "parser",
    "wasm",
    "simd",
    "streaming",
    "partial-json",
    "ai-sdk",
    "llm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamchong/vectorjson.git"
  },
  "homepage": "https://github.com/teamchong/vectorjson#readme",
  "author": "Steven Chong",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.7.0",
    "wasm-opt": "^1.4.0"
  }
}
